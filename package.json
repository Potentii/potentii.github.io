{
	"name": "potentii.github.io",
	"version": "1.0.0",
	"description": "",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Potentii/potentii.github.io.git"
	},
	"bugs": {
		"url": "https://github.com/Potentii/potentii.github.io/issues"
	},
	"homepage": "https://github.com/Potentii/potentii.github.io",
	"scripts": {
		"parcel-serve": "rimraf \"./out/parcel-serve\" && parcel serve \"./\" --target browserDev --no-autoinstall --dist-dir \"./out/parcel-serve/bundle\" --cache-dir \"./out/parcel-serve/cache\" --port 1234",
		"parcel-watch": "rimraf \"./out/parcel-watch\" && parcel watch \"./\" --target browserDev --no-autoinstall --dist-dir \"./out/parcel-watch/bundle\" --cache-dir \"./out/parcel-watch/cache\"",
		"parcel-build": "rimraf \"./out/parcel-build\" && parcel build \"./\" --target browserDev --no-autoinstall --dist-dir \"./out/parcel-build/bundle\" --cache-dir \"./out/parcel-build/cache\" --no-cache",
		"dist-build": "rimraf \"./docs\" && copyfiles \"./out/parcel-build/bundle\" \"./docs\""
	},

	"targets": {
		"browserDev": {
			"context": "browser",
			"isLibrary": false,
			"optimize": false,
			"outputFormat": "esmodule",
			"publicUrl": "./",
			"scopeHoist": false,
			"source": "./index.html",
			"sourceMap": true
		}
	},

	"dependencies": {
		"@babel/runtime": "7.14.0",
		"@potentii/browser-utils": "1.1.0",
		"@potentii/type-utils": "1.0.1",
		"@potentii/logger-js": "1.0.4",
		"axios": "1.3.4",
		"vue": "3.2.19",
		"vue-router": "4.0.11",
		"vuex": "4.0.2",
		"uuid": "9.0.0",
		"process": "0.11.10",
		"util": "0.12.5",
		"os-browserify": "0.3.0",
		"buffer": "5.5.0",
		"events": "3.3.0",
		"stream-http": "3.2.0",
		"https-browserify": "1.0.0",
		"path-browserify": "1.0.1",
		"browserify-zlib": "0.2.0",
		"url": "0.11.0",
		"querystring-es3": "0.2.1",
		"stream-browserify": "3.0.0",
		"assert": "2.0.0",
		"string_decoder": "1.3.0",
		"punycode": "1.4.1"
	},
	"devDependencies": {
		"@babel/core": "7.14.3",
		"@babel/plugin-proposal-nullish-coalescing-operator": "7.14.2",
		"@babel/plugin-proposal-object-rest-spread": "7.14.4",
		"@babel/plugin-proposal-optional-chaining": "7.14.2",
		"@babel/plugin-syntax-import-meta": "7.10.4",
		"@babel/plugin-transform-regenerator": "7.13.15",
		"@babel/plugin-transform-runtime": "7.14.3",
		"@babel/preset-env": "7.14.4",
		"@parcel/transformer-vue": "2.8.3",
		"@vue/component-compiler-utils": "3.2.2",
		"babel-jest": "27.0.2",
		"jest": "27.0.4",
		"parcel": "2.8.3",
		"rimraf": "3.0.2",
		"copyfiles": "2.4.1",
		"vue-hot-reload-api": "2.3.4",
		"vue-template-compiler": "2.6.14"
	}
}
